{
  "name": "Shareview",
  "version": "1.0.0",
  "description": "Shareview review service",
  "main": "index.js",
  "scripts": {
    "server": "better-npm-run server-dev-local-db",
    "jsdoc": "better-npm-run jsdoc"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:xiondlph/shareview.git"
  },
  "author": "ismax <shukhrat@ismax.ru>",
  "betterScripts": {
    "server-dev-local-db": {
      "command": "node app",
      "env": {
        "NODE_ENV": "development",
        "DB_HOST": "localhost",
        "DB_PORT": 27017,
        "DB_AUTH": false,
        "DEBUG": [
          "shareview:cluster",
          "shareview:index",
          "shareview:mongodb",
          "shareview:server"
        ]
      }
    },
    "jsdoc": {
      "command": "jsdoc -c .jsdoc"
    }
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "crypto": "0.0.3",
    "debug": "^2.2.0",
    "express": "^4.14.0",
    "express-session": "^1.14.0",
    "mongodb": "^2.2.4",
    "nedb": "^1.8.0",
    "nodemailer": "^2.5.0",
    "password-generator": "^2.0.2",
    "validator": "^5.5.0"
  },
  "devDependencies": {
    "babel-core": "^6.11.4",
    "babel-preset-es2015": "^6.9.0",
    "better-npm-run": "0.0.10"
  }
}
