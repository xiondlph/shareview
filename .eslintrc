{
    "extends": "airbnb",
    "plugins": [
        "react"
    ],
    "env": {
        "node": true,
        "jest": true
    },
    "rules": {
        "arrow-body-style": ["error", "always"],
        "indent": [
            2,
            4,
            {
                "SwitchCase": 1
            }
        ]
    }
}